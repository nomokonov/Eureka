<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           targetNamespace="urn:EEC:M:HC:SimpleDataObjects:v1.0.3"
           xmlns:hcsdo="urn:EEC:M:HC:SimpleDataObjects:v1.0.3"
           xmlns:bdt="urn:EEC:M:BaseDataTypes:v0.4.6"
           xmlns:csdo="urn:EEC:M:SimpleDataObjects:v0.4.6"
           version="1.0.3">

    <xs:import namespace="urn:EEC:M:BaseDataTypes:v0.4.6" schemaLocation="EEC_M_BaseDataTypes_v0.4.6.xsd"/>
    <xs:import namespace="urn:EEC:M:SimpleDataObjects:v0.4.6" schemaLocation="EEC_M_SimpleDataObjects_v0.4.6.xsd"/>

    <!-- M.HC.SDE.00009: Код АТХ (АТХ. Код) -->
    <xs:element name="ATCCode" type="hcsdo:ATCCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение лекарственного препарата в соответствии с анатомо-терапевтической-химической классификацией</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDT.00004: Код АТХ (АТХ_ Код. Тип) -->
    <xs:simpleType name="ATCCodeType">
        <xs:annotation>
            <xs:documentation>Строка символов, представляющая краткое условное обозначение лекарственного препарата в соответствии с анатомо-терапевтической-химической классификацией</xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:pattern value="[ABCDGHJLMNPRSV]\d{2}[A-Z]{2}\d{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.HC.SDE.00434: Признак подачи заявки на присвоение лекарственному препарату кода АТХ (Признак подачи заявки на присвоение лекарственному препарату кода АТХ. Индикатор) -->
    <xs:element name="ATCIndicator" type="bdt:IndicatorType">
        <xs:annotation>
            <xs:documentation>Признак, определяющий подачу заявки на присвоение лекарственному препарату кода АТХ: 1 – заявка была подана; 0 – заявка не была подана</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00269: Наименование лекарственного препарата в соответствии с АТХ классификацией (Лекарственный препарат в соответствии с АТХ классификацией. Имя) -->
    <xs:element name="ATCName" type="csdo:Name250Type">
        <xs:annotation>
            <xs:documentation>Наименование лекарственного препарата в соответствии с анатомо-терапевтической-химической классификацией</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00230: Код ученой степени (Ученая степень. Код) -->
    <xs:element name="AcademicDegreeCode" type="hcsdo:AcademicDegreeCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение ученой степени</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDT.00049: Код ученой степени (Ученая степень_ Код. Тип) -->
    <xs:simpleType name="AcademicDegreeCodeType">
        <xs:annotation>
            <xs:documentation>Строка символов, представляющая краткое условное обозначение ученой степени</xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:minLength value="1"/>
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.HC.SDE.00112: Наименование ученой степени (Ученая степень. Имя) -->
    <xs:element name="AcademicDegreeName" type="csdo:Name120Type">
        <xs:annotation>
            <xs:documentation>Наименование ученой степени</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00689: Код статуса аттестата аккредитации (Статус аттестата аккредитации. Код) -->
    <xs:element name="AccreditationCertificateStatusCode" type="csdo:DocStatusCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение статуса аттестата аккредитации</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00302: Код вида действия, предпринятого в отношении лекарственного препарата в результате возникновения нежелательной реакции (Вид действия, предпринятого в отношении лекарственного препарата в результате возникновения нежелательной реакции. Код) -->
    <xs:element name="ActionTakenDrugCode" type="hcsdo:ActionTakenDrugCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение вида действия, предпринятого в отношении лекарственного препарата в результате возникновения нежелательной реакции</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDT.00241: Код действия, принятого в отношении лекарственного препарата в результате возникновения нежелательной реакции (Действие, принятое в отношении лекарственного препарата в результате возникновения нежелательной реакции_ Код. Тип) -->
    <xs:simpleType name="ActionTakenDrugCodeType">
        <xs:annotation>
            <xs:documentation>Строка символов, представляющая краткое условное обозначение действия, принятого в отношении лекарственного препарата в результате возникновения нежелательной реакции</xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:enumeration value="01">
                <xs:annotation>
                    <xs:documentation>прекращено применение лекарственного препарата</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="02">
                <xs:annotation>
                    <xs:documentation>уменьшена доза</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="03">
                <xs:annotation>
                    <xs:documentation>увеличена доза</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="04">
                <xs:annotation>
                    <xs:documentation>доза не изменена</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="05">
                <xs:annotation>
                    <xs:documentation>неизвестно</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="06">
                <xs:annotation>
                    <xs:documentation>не установлено</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.HC.SDE.00618: Код активной фармацевтической субстанции (Активная фармацевтическая субстанция. Код) -->
    <xs:element name="ActiveSubstanceCode" type="hcsdo:DrugCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение активной фармацевтической субстанции</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00677: Признак активной фармацевтической субстанции (Признак активной фармацевтической субстанции. Индикатор) -->
    <xs:element name="ActiveSubstanceIndicator" type="bdt:IndicatorType">
        <xs:annotation>
            <xs:documentation>Признак, определяющий является ли лекарственное средство активной фармацевтической субстанцией: 1 – лекарственное средство является активной фармацевтической субстанцией; 0 – лекарственное средство является лекарственным препаратом</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00620: Наименование активной фармацевтической субстанции (Активная фармацевтическая субстанция. Имя) -->
    <xs:element name="ActiveSubstanceName" type="csdo:Name500Type">
        <xs:annotation>
            <xs:documentation>Наименование активной фармацевтической субстанции</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00205: Номер сообщения, оцениваемого вместе с текущим сообщением о нежелательной реакции (Номер сообщения, оцениваемого вместе с текущим сообщением о нежелательной реакции. Идентификатор) -->
    <xs:element name="AdditionDocId" type="csdo:Id50Type">
        <xs:annotation>
            <xs:documentation>Номер сообщения, оцениваемого вместе с текущим сообщением о нежелательной реакции</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00198: Признак доступности дополнительного документа, приложенного отправителем сообщения о нежелательной реакции (Признак доступности дополнительного документа, приложенного отправителем сообщения о нежелательной реакции. Индикатор) -->
    <xs:element name="AdditionDocIndicator" type="bdt:IndicatorType">
        <xs:annotation>
            <xs:documentation>Признак, определяющий доступность дополнительного документа, приложенного отправителем сообщения о нежелательной реакции: 1 – доступен; 0 – не доступен</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00062: Наименование документа, приложенного отправителем сообщения о нежелательной реакции (Документ, приложенный отправителем сообщения о нежелательной реакции. Имя) -->
    <xs:element name="AdditionDocName" type="csdo:Name120Type">
        <xs:annotation>
            <xs:documentation>Наименование документа, приложенного отправителем сообщения о нежелательной реакции на лекарственный препарат</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00564: Дополнительная информация о лекарственном препарате (Дополнительная информация о лекарственном препарате. Текст) -->
    <xs:element name="AdditionInformationDrugText" type="csdo:Text4000Type">
        <xs:annotation>
            <xs:documentation>Дополнительная информация о лекарственном препарате</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00539: Дата получения производителем информации о неблагоприятном событии (инциденте) (Дата получения производителем информации о неблагоприятном событии (инциденте). Дата) -->
    <xs:element name="AdverseIncidentMedicalProductManufacturingDate" type="bdt:DateType">
        <xs:annotation>
            <xs:documentation>Дата получения производителем информации о неблагоприятном событии (инциденте) при применении медицинского изделия</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00548: Количество медицинских изделий (Медицинское изделие. Количество) -->
    <xs:element name="AdverseIncidentMedicalProductQuantity" type="csdo:Quantity4Type">
        <xs:annotation>
            <xs:documentation>Количество медицинских изделий при неблагоприятном событии (инциденте)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00540: Количество пациентов (Пациент. Количество) -->
    <xs:element name="AdverseIncidentPatientQuantity" type="csdo:Quantity4Type">
        <xs:annotation>
            <xs:documentation>Количество пациентов при неблагоприятном событии (инциденте)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00081: Описание нежелательной реакции (Описание нежелательной реакции. Текст) -->
    <xs:element name="AdverseReactionText" type="csdo:Text4000Type">
        <xs:annotation>
            <xs:documentation>Описание нежелательной реакции на лекарственное средство</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00080: Код степени подлинности документа (Cтепень подлинности документа. Код) -->
    <xs:element name="AffirmationKindCode" type="csdo:Code10Type">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение степени подлинности документа</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00083: Наименование степени подлинности документа (Степень подлинности документа. Имя) -->
    <xs:element name="AffirmationKindName" type="csdo:Name120Type">
        <xs:annotation>
            <xs:documentation>Наименование степени подлинности документа</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00164: Код возрастной группы (Возрастная группа. Код) -->
    <xs:element name="AgeGroupCode" type="hcsdo:AgeGroupCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение возрастной группы</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDT.00032: Код возрастной группы (Возрастная группа_ Код. Тип) -->
    <xs:simpleType name="AgeGroupCodeType">
        <xs:annotation>
            <xs:documentation>Строка символов, представляющая краткое условное обозначение возрастной группы</xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:enumeration value="01">
                <xs:annotation>
                    <xs:documentation>новорожденный</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="02">
                <xs:annotation>
                    <xs:documentation>младенец</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="03">
                <xs:annotation>
                    <xs:documentation>ребенок</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="04">
                <xs:annotation>
                    <xs:documentation>подросток</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="05">
                <xs:annotation>
                    <xs:documentation>взрослый</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="06">
                <xs:annotation>
                    <xs:documentation>пожилой</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.HC.SDE.00006: Возраст пациента к моменту возникновения нежелательной реакции (Возраст пациента к моменту возникновения нежелательной реакции. Продолжительность) -->
    <xs:element name="AgeTimeOnsetReaction" type="bdt:DurationType">
        <xs:annotation>
            <xs:documentation>Возраст к началу нежелательной реакции</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00486: Признак, определяющий роль заявителя (Признак, определяющий роль заявителя. Индикатор) -->
    <xs:element name="ApplicantRoleIndicator" type="bdt:IndicatorType">
        <xs:annotation>
            <xs:documentation>Признак, определяющий является ли заявителем производитель медицинских изделий: 1 – заявителем является производитель медицинских изделий; 0 – заявителем является уполномоченный представитель производителя медицинских изделий</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00260: Номер заявления о регистрации лекарственного препарата (Номер заявления о регистрации лекарственного препарата. Идентификатор) -->
    <xs:element name="ApplicationId" type="csdo:Id50Type">
        <xs:annotation>
            <xs:documentation>Номер заявления о регистрации или проведении иных процедур, связанных с регистрацией лекарственного препарата</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00014: Дата подачи заявки на получение сертификата на мастер-файл (Дата подачи заявки на получение сертификата на мастер-файл. Дата) -->
    <xs:element name="ApplicationMasterFileCertificateDocDate" type="bdt:DateType">
        <xs:annotation>
            <xs:documentation>Дата подачи заявки на получение сертификата на мастер-файл</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00012: Номер заявки на получение сертификата на мастер-файл (Номер заявки на получение сертификата на мастер-файл. Идентификатор) -->
    <xs:element name="ApplicationMasterFileCertificateDocId" type="csdo:Id50Type">
        <xs:annotation>
            <xs:documentation>Номер заявки на получение сертификата на мастер-файл</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00545: Код вида применения медицинского изделия (Вид применения медицинского изделия. Код) -->
    <xs:element name="ApplicationMedicalProductCode" type="hcsdo:ApplicationMedicalProductCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение вида применения медицинского изделия</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDT.00219: Код вида применения медицинского изделия (Вид применения медицинского изделия_ Код. Тип) -->
    <xs:simpleType name="ApplicationMedicalProductCodeType">
        <xs:annotation>
            <xs:documentation>Строка символов, представляющая краткое условное обозначение вида применения медицинского изделия</xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:pattern value="\d{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.HC.SDE.00546: Наименование вида применения медицинского изделия (Вид применения медицинского изделия. Имя) -->
    <xs:element name="ApplicationMedicalProductName" type="csdo:Name500Type">
        <xs:annotation>
            <xs:documentation>Наименование вида применения медицинского изделия</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00096: Дата подачи заявления о регистрации лекарственного препарата (Дата подачи заявления о регистрации лекарственного препарата. Дата) -->
    <xs:element name="ApplicationReceiptDate" type="bdt:DateType">
        <xs:annotation>
            <xs:documentation>Дата подачи заявления о регистрации лекарственного препарата или о подтверждении регистрации (перерегистрации) или о внесении изменений в регистрационное досье на лекарственный препарат или о приведении регистрационного досье лекарственного препарата, зарегистрированного до вступления в силу Соглашения о единых принципах и правилах обращения лекарственных средств, в соответствие с требованиями Союза</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00082: Код этапа рассмотрения заявления на регистрацию лекарственного препарата (Этап рассмотрения заявления на регистрацию лекарственного препарата. Код) -->
    <xs:element name="ApplicationStatusCode" type="hcsdo:ApplicationStatusCodeType">
        <xs:annotation>
            <xs:documentation>Кодовое обозначение этапа рассмотрения заявления на регистрацию лекарственного препарата</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDT.00012: Код этапа рассмотрения регистрационного досье (Этап рассмотрения регистрационного досье_ Код. Тип) -->
    <xs:simpleType name="ApplicationStatusCodeType">
        <xs:annotation>
            <xs:documentation>Строка символов, представляющая краткое условное обозначение этапа рассмотрения регистрационного досье</xs:documentation>
        </xs:annotation>
        <xs:restriction base="bdt:CodeType">
            <xs:enumeration value="01">
                <xs:annotation>
                    <xs:documentation>прием заявки</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="02">
                <xs:annotation>
                    <xs:documentation>экспертиза досье</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="03">
                <xs:annotation>
                    <xs:documentation>апробация методов контроля качества</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="04">
                <xs:annotation>
                    <xs:documentation>инспекция производства</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="05">
                <xs:annotation>
                    <xs:documentation>подготовка экспертного отчета</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="06">
                <xs:annotation>
                    <xs:documentation>одобрение экспертного отчета</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="07">
                <xs:annotation>
                    <xs:documentation>отказ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="99">
                <xs:annotation>
                    <xs:documentation>другое</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <!-- M.HC.SDE.00698: Наименование этапа рассмотрения заявления на регистрацию лекарственного препарата (Этап рассмотрения заявления на регистрацию лекарственного препарата. Имя) -->
    <xs:element name="ApplicationStatusName" type="csdo:Name500Type">
        <xs:annotation>
            <xs:documentation>Наименование этапа рассмотрения заявления на регистрацию лекарственного препарата</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.SDE.00488: Код причины невозможности рассмотрения заявления (Причина невозможности рассмотрения заявления. Код) -->
