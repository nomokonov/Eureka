<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           targetNamespace="urn:EEC:M:HC:ComplexDataObjects:v1.0.3"
           xmlns:hccdo="urn:EEC:M:HC:ComplexDataObjects:v1.0.3"
           xmlns:ccdo="urn:EEC:M:ComplexDataObjects:v0.4.6"
           xmlns:hcsdo="urn:EEC:M:HC:SimpleDataObjects:v1.0.3"
           xmlns:csdo="urn:EEC:M:SimpleDataObjects:v0.4.6"
           version="1.0.3">

    <xs:import namespace="urn:EEC:M:ComplexDataObjects:v0.4.6" schemaLocation="EEC_M_ComplexDataObjects_v0.4.6.xsd"/>
    <xs:import namespace="urn:EEC:M:HC:SimpleDataObjects:v1.0.3" schemaLocation="EEC_M_HC_SimpleDataObjects_v1.0.3.xsd"/>
    <xs:import namespace="urn:EEC:M:SimpleDataObjects:v0.4.6" schemaLocation="EEC_M_SimpleDataObjects_v0.4.6.xsd"/>

    <!-- M.HC.CDE.00652: Сведения об аттестате аккредитации (Аттестат аккредитации. Детали) -->
    <xs:element name="AccreditationCertificateDetails" type="hccdo:AccreditationCertificateDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения об аттестате аккредитации</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00689: Сведения об аттестате аккредитации (Аттестат аккредитации_ Детали. Тип) -->
    <xs:complexType name="AccreditationCertificateDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения об аттестате аккредитации</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:DocId">
                <xs:annotation>
                    <xs:documentation>Номер аттестата аккредитации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:DocCreationDate">
                <xs:annotation>
                    <xs:documentation>Дата выдачи аттестата аккредитации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:DocValidityDate" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата окончания действия аттестата аккредитации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hcsdo:AccreditationCertificateStatusCode"/>
            <xs:element ref="csdo:DescriptionText" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>описание области аккредитации испытательной лаборатории (центра)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00587: Сведения об активной фармацевтической субстанции, используемой при производстве лекарственного препарата (Сведения об активной фармацевтической субстанции, используемой при производстве лекарственного препарата. Детали) -->
    <xs:element name="ActiveIngredientDetails" type="hccdo:ActiveIngredientDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения об активной фармацевтической субстанции, используемой при производстве лекарственного препарата</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00659: Сведения об активной фармацевтической субстанции, используемой при производстве лекарственного препарата (Сведения об активной фармацевтической субстанции, используемой при производстве лекарственного препарата_ Детали. Тип) -->
    <xs:complexType name="ActiveIngredientDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения об активной фармацевтической субстанции, используемой при производстве лекарственного препарата</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="hcsdo:DrugTradeName">
                <xs:annotation>
                    <xs:documentation>Наименование активной фармацевтической субстанции</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hccdo:DrugNameDetails" minOccurs="0"/>
            <xs:element ref="hccdo:ManufacturingAuthorizationHolderDetailsV2">
                <xs:annotation>
                    <xs:documentation>Сведения о производителе активной фармацевтической субстанции, осуществляющем выпускающий контроль качества</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00030: Сведения об активной фармацевтической субстанции в составе лекарственного препарата (Активная фармацевтическая субстанция в составе лекарственного препарата. Детали) -->
    <xs:element name="ActiveSubstanceDetails" type="hccdo:ActiveSubstanceDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения об активной фармацевтической субстанции в составе лекарственного препарата</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00730: Сведения об активной фармацевтической субстанции в составе лекарственного препарата (Активная фармацевтическая субстанция в составе лекарственного препарата_ Детали. Тип) -->
    <xs:complexType name="ActiveSubstanceDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения об активной фармацевтической субстанции в составе лекарственного препарата</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="hcsdo:ActiveSubstanceCode" minOccurs="0"/>
            <xs:element ref="hcsdo:ActiveSubstanceName" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00072: Сведения о дополнительных документах, принадлежащих отправителю сообщения о нежелательной реакции (Сведения о дополнительных документах, принадлежащих отправителю сообщения о нежелательной реакции. Детали) -->
    <xs:element name="AdditionalAvailableDocumentDetails" type="hccdo:AdditionalAvailableDocumentDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о дополнительных документах, принадлежащих отправителю сообщения о нежелательной реакции</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00046: Сведения о дополнительных документах, принадлежащих отправителю сообщения о нежелательной реакции (Сведения о дополнительных документах, принадлежащих отправителю сообщения о нежелательной реакции_ Детали. Тип) -->
    <xs:complexType name="AdditionalAvailableDocumentDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения о дополнительных документах, принадлежащих отправителю сообщения о нежелательной реакции</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="hcsdo:AdditionDocIndicator"/>
            <xs:element ref="hcsdo:AdditionDocName" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00034: Сведения об обучении (подготовке), повышении квалификации и проверке знаний (Обучение (подготовка), повышение квалификации и проверке знаний. Детали) -->
    <xs:element name="AdditionalEducationDetails" type="hccdo:AdditionalEducationDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о дополнительном профессиональном образовании или повышении квалификации уполномоченного лица</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00072: Сведения о дополнительном образовании (Сведения о дополнительном образовании_ Детали. Тип) -->
    <xs:complexType name="AdditionalEducationDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения о дополнительном образовании</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:BusinessEntityName">
                <xs:annotation>
                    <xs:documentation>наименование учебного учреждения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:StartDate">
                <xs:annotation>
                    <xs:documentation>дата начала обучения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:EndDate">
                <xs:annotation>
                    <xs:documentation>дата окончания обучения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hcsdo:EducationDocName">
                <xs:annotation>
                    <xs:documentation>наименование документа о дополнительном образовании</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hcsdo:EducationDocSeriesId" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>серия документа о дополнительном образовании</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hcsdo:EducationDocNumberId" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>номер документа о дополнительном образовании</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hcsdo:SpecialityCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Кодовое обозначение специальности в соответствии с документом о дополнительном образовании</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hcsdo:SpecialityName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование специальности в соответствии с документом о дополнительном образовании</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hcsdo:QualificationDegreeCode" minOccurs="0"/>
            <xs:element ref="hcsdo:QualificationDegreeName" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00106: Сведения о распорядительном документе, на основании которого медицинской организации разрешено проведение испытаний (исследований) медицинских изделий (Сведения о распорядительном документе, на основании которого медицинской организации разрешено проведение испытаний (исследований) медицинских изделий. Детали) -->
    <xs:element name="AdministrativeDocumentDetails" type="hccdo:MedicineDocDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о распорядительном документе, на основании которого медицинской организации разрешено проведение клинических и (или) клинико-лабораторных испытаний (исследований) медицинских изделий</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDE.00279: Сведения о неблагоприятном событии (инциденте) (Сведения о неблагоприятном событии (инциденте). Детали) -->
    <xs:element name="AdverseIncidentMedicalProductDetails" type="hccdo:AdverseIncidentMedicalProductDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о неблагоприятном событии (инциденте)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00260: Сведения о неблагоприятном событии (инциденте) (Сведения о неблагоприятном событии (инциденте)_ Детали. Тип) -->
    <xs:complexType name="AdverseIncidentMedicalProductDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения о неблагоприятном событии (инциденте), произошедшем при использовании медицинского изделия</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="hcsdo:ReportCorrectInfluenceTypeCode"/>
            <xs:element ref="csdo:DocId" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>номер отчета медицинской организации – пользователя о неблагоприятном событии (инциденте)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:DocCreationDate">
                <xs:annotation>
                    <xs:documentation>дата отчета медицинской организации – пользователя о неблагоприятном событии (инциденте)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:EventDate">
                <xs:annotation>
                    <xs:documentation>дата неблагоприятного события (инцидента)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:DescriptionText">
                <xs:annotation>
                    <xs:documentation>описание неблагоприятного события (инцидента)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hcsdo:AdverseIncidentMedicalProductManufacturingDate"/>
            <xs:element ref="hcsdo:AdverseIncidentPatientQuantity" minOccurs="0"/>
            <xs:element ref="hcsdo:AdverseIncidentMedicalProductQuantity" minOccurs="0"/>
            <xs:element ref="ccdo:SubjectAddressDetails" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>текущее место нахождения медицинского изделия</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hcsdo:UserKindTimeAdverseIncidentCode" minOccurs="0"/>
            <xs:element ref="hcsdo:UserKindTimeAdverseIncidentName" minOccurs="0"/>
            <xs:element ref="hcsdo:ApplicationMedicalProductCode" minOccurs="0"/>
            <xs:element ref="hcsdo:ApplicationMedicalProductName" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00033: Сведения о заверении документа (Сведения о заверении документа. Детали) -->
    <xs:element name="AffirmationDocDetails" type="hccdo:AffirmationDocDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о заверении документа</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00017: Сведения о заверении документа (Сведения о заверении документа_ Детали. Тип) -->
    <xs:complexType name="AffirmationDocDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения о заверении документа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="hcsdo:AffirmationKindCode" minOccurs="0"/>
            <xs:element ref="hcsdo:AffirmationKindName" minOccurs="0"/>
            <xs:element ref="hccdo:AffirmationOrganizationDetails" minOccurs="0"/>
            <xs:element ref="ccdo:FullNameDetails" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>сведения о фамилии, имени и отчестве лица, заверившего документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hcsdo:PositionCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>кодовое обозначение должности лица, заверившего документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:PositionName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>наименование должности лица, заверившего документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="csdo:EventDate" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>дата заверения документа</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00026: Сведения об организации, заверившей документ (Организация, заверившая документ. Детали) -->
    <xs:element name="AffirmationOrganizationDetails" type="ccdo:BusinessEntityDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения об организации, заверившей документ</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDE.00081: Информация о возрасте пациента или родителя пациента (Информация о возрасте пациента или родителя пациента. Детали) -->
    <xs:element name="AgeInformationDetails" type="hccdo:AgeInformationDetailsType">
        <xs:annotation>
            <xs:documentation>Информация о возрасте пациента или родителя пациента</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00056: Информация о возрасте пациента или родителя пациента (Информация о возрасте пациента или родителя пациента_ Детали. Тип) -->
    <xs:complexType name="AgeInformationDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие информацию о возрасте пациента или родителя пациента</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="csdo:BirthDate" minOccurs="0"/>
            <xs:element ref="hcsdo:AgeTimeOnsetReaction" minOccurs="0"/>
            <xs:element ref="hcsdo:GestationPeriodDuration" minOccurs="0"/>
            <xs:element ref="hcsdo:AgeGroupCode" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00303: Документ в формате XML (XML-документ. Детали) -->
    <xs:element name="AnyDetails" type="hccdo:AnyDetailsType">
        <xs:annotation>
            <xs:documentation>XML-документ произвольной структуры</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00274: XML-документ (XML-документ_ Детали. Тип) -->
    <xs:complexType name="AnyDetailsType">
        <xs:annotation>
            <xs:documentation>XML-документ произвольной структуры</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:any namespace="##any" processContents="strict">
                <xs:annotation>
                    <xs:documentation>XML-документ произвольной структуры</xs:documentation>
                </xs:annotation>
            </xs:any>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00505: Сведения о сотруднике организации, выступающем от имени заявителя (Сотрудник организации, выступающий от имени заявителя. Детали) -->
    <xs:element name="ApplicantEmployeeDetails" type="hccdo:OfficerDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о сотруднике организации, выступающем от имени заявителя</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDE.00015: Заявление о соответствии производства правилам надлежащей производственной практики (Заявление о соответствии производства правилам надлежащей производственной практики. Детали) -->
    <xs:element name="ApplicationGMPCorrespondenceDetails" type="hccdo:DrugRegistrationDocDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о заявлении о соответствии производства правилам надлежащей производственной практики</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDE.00029: Сведения о заявке на получение сертификата на мастер-файл (Сведения о заявке на получение сертификата на мастер-файл. Детали) -->
    <xs:element name="ApplicationMasterFileCertificateDetails" type="hccdo:ApplicationMasterFileCertificateDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о заявке на получение сертификата на мастер-файл</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00078: Сведения о заявке на получение сертификата на мастер-файл (Сведения о заявке на получение сертификата на мастер-файл_ Детали. Тип) -->
    <xs:complexType name="ApplicationMasterFileCertificateDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения о заявке на получение сертификата на мастер-файл</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="hcsdo:ApplicationMasterFileCertificateDocId" minOccurs="0"/>
            <xs:element ref="hcsdo:ApplicationMasterFileCertificateDocDate" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>дата подачи заявки на получение сертификата на мастер-файл</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00912: Сведения об этапе рассмотрения заявления на регистрацию лекарственного препарата (Сведения об этапе рассмотрения заявления на регистрацию лекарственного препарата. Детали) -->
    <xs:element name="ApplicationStatusDetails" type="hccdo:ApplicationStatusDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения об этапе рассмотрения заявления на регистрацию лекарственного препарата</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- M.HC.CDT.00912: Сведения об этапе рассмотрения заявления на регистрацию лекарственного препарата (Сведения об этапе рассмотрения заявления на регистрацию лекарственного препарата_ Детали. Тип) -->
    <xs:complexType name="ApplicationStatusDetailsType">
        <xs:annotation>
            <xs:documentation>Компоненты, представляющие сведения об этапе рассмотрения заявления на регистрацию лекарственного препарата</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="hcsdo:ApplicationStatusCode"/>
            <xs:element ref="hcsdo:ApplicationStatusName" minOccurs="0"/>
            <xs:element ref="csdo:EventDate">
                <xs:annotation>
                    <xs:documentation>Дата изменения этапа рассмотрения регистрационного досье</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- M.HC.CDE.00682: Сведения о действии, предпринятом государственным органом исполнительной власти (Действие, предпринятое государственным органом исполнительной власти. Детали) -->
    <xs:element name="AuthorityActionDetails" type="hccdo:AuthorityActionDetailsType">
        <xs:annotation>
            <xs:documentation>Сведения о действии, предпринятом государственным органом исполнительной власти, в компетенцию которого входит государственный надзор (контроль) за лекарственными средствами</xs:documentation>
